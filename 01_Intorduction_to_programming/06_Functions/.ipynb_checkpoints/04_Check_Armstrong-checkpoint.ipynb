   "source": [
    "def isArmstrong(n):\n",
    "    power = len(str(n))\n",
    "    sum = 0\n",
    "    rem = 0\n",
    "    while n>=1:\n",
    "        rem = n % 10\n",
    "        sum = sum + rem**power\n",
    "        n=n//10\n",
    "    return sum\n",
    "    \n",
    "n = int(input())\n",
    "result = isArmstrong(n)\n",
    "if result == n:\n",
    "    print(\"true\")\n",
    "else:\n",
    "    print(\"false\")"
   ]
  }
