    "def printTable(start,end,step):\n",
    "    for i in range(start, end, step):\n",
    "        c = int((i-32)*5/9)\n",
    "        print(i,\"\\t\",c)    \n",
    "\n",
    "s = int(input())\n",
    "e = int(input())\n",
    "step = int(input())\n",
    "printTable(s,e,step)\n",
    "\n",
