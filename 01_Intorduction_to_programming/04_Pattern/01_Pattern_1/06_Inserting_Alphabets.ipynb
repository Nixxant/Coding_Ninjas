   "source": [
    "N = int(input())\n",
    "0 <= N <= 26\n",
    "i = 1\n",
    "x = ord('A')\n",
    "\n",
    "\n",
    "while i <= N:\n",
    "    j = 1\n",
    "    s = int(N)\n",
    "    while j <= i:\n",
    "        print(chr(s+x-i), end = \"\")\n",
    "        s = s + 1\n",
    "        j = j + 1\n",
    "        \n",
    "    print()\n",
    "    i = i + 1"
   ]
