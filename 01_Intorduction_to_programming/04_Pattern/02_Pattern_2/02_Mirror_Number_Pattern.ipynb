   "source": [
    "N = int(input())\n",
    "0 <= N <= 50\n",
    "i = 1\n",
    "while i <= N:\n",
    "    spaces = 1\n",
    "    while spaces <= N-i:\n",
    "        print(\" \", end = \"\")\n",
    "        spaces = spaces + 1\n",
    "    stars = 1\n",
    "    while stars <= i:\n",
    "        print( stars, end = \"\")\n",
    "        stars = stars + 1\n",
    "    print()\n",
    "    i = i + 1"
   ]
  }
