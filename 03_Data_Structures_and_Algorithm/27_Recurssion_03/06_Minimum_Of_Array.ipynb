    "def find_min(arr,min_num = 999999999):\n",
    "    if len(arr) == 0 :\n",
    "        print(min_num)\n",
    "        return\n",
    "    if arr[0] < min_num:\n",
    "        min_num = arr[0]\n",
    "    find_min(arr[1:],min_num)\n",
    "arr = [1,2,3,4,-1,0,-2,-4,5,6]\n",
    "find_min(arr)"
