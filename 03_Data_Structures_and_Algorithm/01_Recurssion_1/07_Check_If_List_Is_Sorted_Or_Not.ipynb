    "def checkList(li):\n",
    "    l = len(li)\n",
    "    if l <= 1:\n",
    "        return True\n",
    "    if li[0] <= li[1] and checkList(li[1:]):\n",
    "        return True\n",
    "\n",
    "    else:\n",
    "        return False\n",
    "    \n",
    "li = [0,0,0]\n",
    "print(checkList(li))\n",
    "    "
