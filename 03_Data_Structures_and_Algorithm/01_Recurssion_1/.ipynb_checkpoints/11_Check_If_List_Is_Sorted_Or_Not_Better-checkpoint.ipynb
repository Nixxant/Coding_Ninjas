    "def isSortedBetter(li,si):\n",
    "    l = len(li)\n",
    "    if si == l-1 or si == l:\n",
    "        return True\n",
    "    if li[si] > li[si+1]:\n",
    "        return False\n",
    "    return isSortedBetter(li,si+1)\n",
    "\n",
    "li = [1,2,3,4,5,6,7,8,8,76,6]\n",
    "print(isSortedBetter(li,0))\n",
    "        "
   ]
