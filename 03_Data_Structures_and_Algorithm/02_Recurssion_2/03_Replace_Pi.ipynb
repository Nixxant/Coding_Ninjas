   "source": [
    "def replacePi(s):\n",
    "    if len(s) == 0:\n",
    "        return s\n",
    "    if s[0] == \"P\" and s[1] == 'i':\n",
    "        return \"3.14\"+replacePi(s[2:])\n",
    "    else:\n",
    "        return s[0]+replacePi(s[1:])"
   ]
  },
