   "source": [
    "def reverseLL_iterative(head):\n",
    "    if head is None or head.next is None:\n",
    "        return head\n",
    "    curr = head\n",
    "    prev = None\n",
    "    while curr is not None:\n",
    "        tmp = curr.next\n",
    "        curr.next = prev\n",
    "        prev = curr\n",
    "        curr = tmp\n",
    "    return prev\n",
    "        "
   ]
  }
