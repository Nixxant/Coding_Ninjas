   "source": [
    "def __removePadding(self):\n",
    "    padded_info = text[:8]\n",
    "    extra_padding = int(padded_info, 2)\n",
    "\n",
    "    text = text[8:]\n",
    "\n",
    "    text_after_padding_removed = text[:-1* extra_padding]\n",
    "\n",
    "    return text_after_padding_removed"
   ]
  }
