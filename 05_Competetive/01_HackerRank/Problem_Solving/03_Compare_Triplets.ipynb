   "source": [
    "def compareTriplets(a,b):\n",
    "    a = 0\n",
    "    b = 0\n",
    "    for i in range(3):\n",
    "        if a[i] > b[i]:\n",
    "            a += 1\n",
    "        elif b[i] > a[i]:\n",
    "            b += 1\n",
    "    return a, b"
   ]
  }
