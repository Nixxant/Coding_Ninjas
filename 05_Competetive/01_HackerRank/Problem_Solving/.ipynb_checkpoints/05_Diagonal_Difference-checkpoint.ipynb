   "source": [
    "def diagonalDifference(arr):\n",
    "    left = 0\n",
    "    right = 0\n",
    "    n = len(arr)\n",
    "    for i in range(n):\n",
    "        left += arr[i][i]\n",
    "        right += arr[i][n-i-1]\n",
    "    return abs(left - right)"
   ]
